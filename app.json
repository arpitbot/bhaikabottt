{
    "name": "AdityaPlayer",
    "logo": "https://te.legra.ph/file/2e2ddeb33674ba956148f.jpg",
    "description": "An Advanced Premium Music Bot For Telegram Groups VC.",
    "keywords": ["music", "voice chat", "telegram"],
    "repository": "https://github.com/itsadityaxd/adityaplayer",
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "Your App ID",
            "required": true
        },
        "API_HASH": {
            "description": "Your App Hash",
            "required": true
        },
        "BOT_USERNAME": {
            "description": "Put Bot Username Without '@' Ex:- adityaprobot",
            "required": true,
            "value": ""
        },
        "BOT_TOKEN": {
            "description": "Put Bot Token Of Your Bot",
            "required": true
        },
        "BG_IMAGE": {
            "description": "Custom Thumbnail (Leave it if You Don't Want)",
            "required": false,
            "value": "https://te.legra.ph/file/b68d40fc2759ea46277b3.png"
       },
        "DURATION_LIMIT": {
            "description": "Maximum Duration For Song",
            "required": true,
            "value": "15"
        },
        "STRING_SESSION": {
            "description": "Pyrogram String Session",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Put Your User ID (Separted By Space)",
            "required": true,
            "value": "1282754256"
        }
    },
    "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ]
}
